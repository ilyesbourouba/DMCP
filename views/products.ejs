        <script src="../assets/js/plugins/dropzone.min.js"></script>
        <script type="text/javascript">
            Dropzone.autoDiscover = false;
            var drop = document.getElementById('dropzone')
            var myDropzone = new Dropzone(drop, {
                url: "/file/post",
                addRemoveLinks: true

            });
        </script>
        <script>
            // CREATE
            $('#add_Prod').click(function() {
                var formData = new FormData();

                formData.append("name_fr", $('#name_fr').val());
                formData.append("name_en", $('#name_en').val());
                formData.append("name_ar", $('#name_ar').val());

                formData.append("description_fr", $('#description_fr').val());
                formData.append("description_ar", $('#description_ar').val());
                formData.append("description_en", $('#description_en').val());

                formData.append("category", $('#Category').val());
                formData.append("price", $('#price').val());
                formData.append("stock", $('#stock').val());

                formData.append("best_selling", $('#best_selling').is(':checked'));
                formData.append("top_rating", $('#top_rating').is(':checked'));
                formData.append("new_product", $('#new_product').is(':checked'));

                var images = myDropzone.files;
                images.forEach(image => {
                    formData.append("images", image);
                });

                $.ajax({
                    url: "/products/add",
                    method: "POST",
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function(response) {
                        Swal.fire({
                            icon: "success",
                            title: "Product Added!",
                            text: "The Product has been added successfully.",
                            confirmButtonText: "OK"
                        }).then(() => {
                            location.reload();
                        });
                    },
                    error: function(response) {
                        console.log(response)
                        Swal.fire({
                            icon: "error",
                            title: "Error",
                            text: "Something went wrong. Please try again.",
                            confirmButtonText: "OK"
                        });
                    }
                });
            });
        </script>

        <script>
            // DELETE
            $("a[id='delete_btn']").click(function() {

                let id_delete = $(this).attr('data-id');
                let nom = $(this).attr("data-nom");

                $("#delete_nom").html(nom);
                $("#delete_id").val(id_delete);

            });

            $("button[id='modal_delete']").click(function() {
                let id = $("#delete_id").val();

                $.ajax({
                    url: '/products/delete/',
                    data: {
                        id
                    },
                    method: 'DELETE',
                    success: function(response) {
                        Swal.fire({
                            icon: 'success',
                            title: 'Product Deleted!',
                            text: 'The Product has been deleted successfully.',
                            confirmButtonText: 'OK'
                        }).then(() => {
                            location.reload();
                        });
                    },
                    error: function() {
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Something went wrong. Please try again.',
                            confirmButtonText: 'OK'
                        });
                    }
                });
            });
        </script>
        <script>
            // EDIT
            Dropzone.autoDiscover = false;
            var edit_drop = document.getElementById('edit_dropzone')
            var edit_Dropzone = new Dropzone(edit_drop, {
                url: "/file/post",
                addRemoveLinks: true

            });
            $("a[id='edit_btn']").click(function() {

                $("#image_viewer").empty();

                let id_edit = $(this).attr('data-id');

                let nom_fr = $(this).attr("data-name_fr");
                let nom_ar = $(this).attr("data-name_ar");
                let nom_en = $(this).attr("data-name_en");

                let description_fr = $(this).attr("data-description_fr");
                let description_ar = $(this).attr("data-description_ar");
                let description_en = $(this).attr("data-description_en");

                let price = $(this).attr("data-price");
                let stock = $(this).attr("data-stock");
                let category_id = $(this).attr("data-category_id");
                let category_name = $(this).attr("data-category_name");

                let best_selling = $(this).attr("data-best_selling");
                let new_product = $(this).attr("data-new_product");
                let top_rating = $(this).attr("data-top_rating");
                let images = $(this).attr("data-images");

                $("#edit_id").val(id_edit);
                $("#edit_name_fr").val(nom_fr);
                $("#edit_name_ar").val(nom_ar);
                $("#edit_name_en").val(nom_en);

                $("#edit_description_fr").val(description_fr);
                $("#edit_description_ar").val(description_ar);
                $("#edit_description_en").val(description_en);

                $("#edit_price").val(price);
                $("#edit_stock").val(stock);
                $("#edit_category_id").val(category_id);
                $("#edit_category_name").val(category_name);

                $("#edit_best_selling").prop("checked", best_selling === "1" || best_selling === "true");
                $("#edit_top_prod").prop("checked", top_rating === "1" || top_rating === "true");
                $("#edit_new_prod").prop("checked", new_product === "1" || new_product === "true");

                if (images) {
                    let imagesArray = images.split(",");

                    imagesArray.forEach(image => {
                        let imageHtml = `
                            <div class="col-md-4">
                                <div class="card card-profile card-plain image-card">
                                    <div class="card-body text-center bg-white shadow border-radius-lg p-3" style="border :2px solid #82d616">
                                        <a href="javascript:;">
                                            <img class="w-100 border-radius-md" src="${image.trim()}" alt="Product Image">
                                        </a>
                                        <p class="mt-3 mb-1 d-md-block d-none">image</p>
                                        <a href="#" class="mb-0 text-xs font-weight-bolder text-danger text-gradient text-uppercase delete-image delete-btn" data-image="${image.trim()}">delete</a>
                                    </div>
                                </div>
                            </div>
                        `;

                        $("#image_viewer").append(imageHtml);
                    });
                }
            });

            // Delete image
            $(document).on("click", ".delete-image", function(e) {
                e.preventDefault();

                let imageElement = $(this).closest(".col-md-4"); // Récupère le conteneur de l'image
                let imageName = $(this).attr("data-image"); // Récupère le nom de l'image
                let productId = $("#edit_id").val(); // ID du produit

                console.log("imageName", imageName);
                console.log("productId", productId);

                $.ajax({
                    url: "/products/deleteimage", // Endpoint pour supprimer l'image côté serveur
                    type: "DELETE",
                    data: {
                        image: imageName,
                        id: productId
                    },
                    success: function(response) {
                        if (response.success) {
                            imageElement.remove(); // Supprime l'image du DOM après succès
                        } else {
                            alert("Erreur lors de la suppression de l'image.");
                        }
                    },

                    success: function(response) {

                        Swal.fire({
                            icon: "success",
                            title: "Product Added!",
                            text: "The Product has been added successfully.",
                            confirmButtonText: "OK"
                        }).then(() => {
                            imageElement.remove();
                        });
                    },
                    error: function(response) {
                        console.log(response)
                        Swal.fire({
                            icon: "error",
                            title: "Error",
                            text: "Something went wrong. Please try again.",
                            confirmButtonText: "OK"
                        });
                    }
                });
            });

            //update the product
            $("button[id='modal_edit']").click(function() {
                var formData = new FormData();

                formData.append("id", $('#edit_id').val());

                formData.append("name_fr", $('#edit_name_fr').val());
                formData.append("name_ar", $('#edit_name_ar').val());
                formData.append("name_en", $('#edit_name_en').val());

                formData.append("desc_fr", $('#edit_description_fr').val());
                formData.append("desc_ar", $('#edit_description_ar').val());
                formData.append("desc_en", $('#edit_description_en').val());

                formData.append("category", $('#edit_Category').val());
                formData.append("price", $('#edit_price').val());
                formData.append("stock", $('#edit_stock').val());

                formData.append("best_selling", $('#edit_best_selling').is(':checked'));
                formData.append("new_product", $('#edit_new_prod').is(':checked'));
                formData.append("top_rating", $('#edit_top_prod').is(':checked'));

                var images = edit_Dropzone.files;
                images.forEach(image => {
                    formData.append("images", image);
                });

                $.ajax({
                    url: "/products/update",
                    method: "PUT",
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function(response) {
                        Swal.fire({
                            icon: "success",
                            title: "Product Updated!",
                            text: "The Product has been updated successfully.",
                            confirmButtonText: "OK"
                        }).then(() => {
                            location.reload();
                        });
                    },
                    error: function(response) {
                        console.log(response);

                        Swal.fire({
                            icon: "error",
                            title: "Error",
                            text: "Something went wrong. Please try again.",
                            confirmButtonText: "OK"
                        });
                    }
                });
            });
        </script>
