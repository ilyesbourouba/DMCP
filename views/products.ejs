<%- include('./partials/sidebar'); %>
    <%- include('./partials/header'); %>
        <style>
            .image-card {
                width: 250px;
                height: 320px;
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: space-between;
                border-radius: 10px;
                box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
                padding: 10px;
                background: #d0d1d3;
                text-align: center;
            }
            
            .image-card img {
                width: 100%;
                height: 200px;
                object-fit: cover;
                border-radius: 8px;
            }
            
            .image-card .delete-btn {
                color: red;
                font-weight: bold;
                cursor: pointer;
            }
        </style>

        <div class="container-fluid py-4">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <!-- Card header -->
                        <div class="card-header pb-0">
                            <div class="d-lg-flex">
                                <div>
                                    <h5 class="mb-0">All Products</h5>
                                </div>
                                <div class="ms-auto my-auto mt-lg-0 mt-4">
                                    <div class="ms-auto my-auto">
                                        <a href="#" class="btn btn-primary btn-sm mb-0" data-bs-toggle="modal" data-bs-target="#import">+&nbsp; New Product</a>


                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body px-0 pb-0">
                            <div class="table-responsive">
                                <div class="dataTable-wrapper dataTable-loading no-footer sortable searchable fixed-columns">

                                    <div class="dataTable-container">
                                        <table class="table align-items-center mb-0" id="datatable">
                                            <thead>
                                                <tr>
                                                    <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Name</th>
                                                    <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Category</th>
                                                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Price</th>
                                                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Stock</th>
                                                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Best selling</th>
                                                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">New products</th>
                                                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Top rating</th>
                                                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Operations</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% products.map(product=>{ %>
                                                    <tr>
                                                        <td>
                                                            <div class="d-flex px-2 py-1">
                                                                <div class="d-flex flex-column justify-content-center">
                                                                    <h6 class="mb-0 text-sm">
                                                                        <%= product.name_fr %>
                                                                    </h6>
                                                                </div>
                                                            </div>
                                                        </td>
                                                        <td>
                                                            <p class="text-xs font-weight-bold mb-0">
                                                                <%= product.category_name_fr  %>
                                                            </p>
                                                        </td>
                                                        <td class="align-middle text-center text-sm">
                                                            <span class="text-secondary text-xs font-weight-bold"><%= product.price%> DA</span>
                                                        </td>
                                                        <td class="align-middle text-center">
                                                            <span class="text-secondary text-xs font-weight-bold"><%= product.stock %> piece</span>
                                                        </td>
                                                        <td class="align-middle text-center text-sm">
                                                            <% let best_color=""; if(product.best_selling) {best_color="success"} else {best_color="danger"}  %>
                                                                <span class="badge badge-sm bg-gradient-<%= best_color %>">&nbsp;&nbsp;</span>
                                                        </td>
                                                        <td class="align-middle text-center text-sm">
                                                            <% let new_prod_color=""; if(product.new_product) {new_prod_color="success"} else {new_prod_color="danger"}  %>
                                                                <span class="badge badge-sm bg-gradient-<%= new_prod_color %>">&nbsp;&nbsp;</span>
                                                        </td>
                                                        <td class="align-middle text-center text-sm">
                                                            <% let top_prod_color=""; if(product.top_rating) {top_prod_color="success"} else {top_prod_color="danger"}  %>
                                                                <span class="badge badge-sm bg-gradient-<%= top_prod_color %>">&nbsp;&nbsp;</span>
                                                        </td>
                                                        <td class="align-middle">
                                                            <center>
                                                                <a data-id="<%= product.id %>" data-nom='<%= product.name_fr %>' class="text-secondary font-weight-bold text-xs" id="delete_btn" data-bs-target="#delete" href="" data-bs-toggle="modal" data-toggle="tooltip" data-original-title="delete product">
                                                                    <i class="fas fa-trash-alt" style="font-size: large; color: rgb(187, 18, 18);" class="pe-2 btn-tooltip" data-bs-toggle="tooltip" data-bs-placement="top" data-container="body" title="delete" data-animation="true"></i>
                                                                </a>
                                                                <a data-id="<%= product.id %>" data-name_fr='<%= product.name_fr %>' data-name_ar='<%= product.name_ar %>' data-name_en='<%= product.name_en %>' data-description_fr='<%= product.desc_fr %>' data-description_ar='<%= product.desc_ar %>' data-description_en='<%= product.desc_en %>'
                                                                    data-price='<%= product.price %>' data-stock='<%= product.stock %>' data-best_selling='<%= product.best_selling %>' data-new_product='<%= product.new_product %>' data-top_rating='<%= product.top_rating %>'
                                                                    data-category_id='<%= product.category_id %>' data-category_name='<%= product.category_namef_r %>' data-images="<%= product.images %>" id="edit_btn" href="" data-bs-toggle="modal" data-bs-target="#edit"
                                                                    class="ms-3 text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit product">
                                                                    <i class="fas fa-edit" style="font-size: large; color: rgb(23, 23, 177);" data-bs-toggle="tooltip" data-bs-placement="top" title="edit" data-container="body" data-animation="true"></i>
                                                                </a>

                                                            </center>
                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>

