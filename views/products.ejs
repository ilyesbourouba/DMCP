                                <div class="input-group mb-3">
                                    <input id="edit_name_fr" name="edit_name_fr" type="text" class="form-control" placeholder="name..." aria-label="name_fr" aria-describedby="name-addon">
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <label for="name_ar">Nom arabe</label>
                                <div class="input-group mb-3">
                                    <input id="edit_name_ar" name="edit_name_ar" type="text" class="form-control" placeholder="name..." aria-label="name_ar" aria-describedby="name-addon">
